{
  "name": "solana-earphone-frontend",
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://Jacklovey.github.io",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "preview": "vite preview",
    "test": "jest",
    "test:unit": "jest --testMatch=\"**/__tests__/**/*.test.{js,jsx}\"",
    "test:integration": "jest --testMatch=\"**/__tests__/**/*integration*.{js,jsx}\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:unit": "jest --coverage --testMatch=\"**/__tests__/**/*.test.{js,jsx}\"",
    "test:coverage:check": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":75,\"functions\":85,\"lines\":80,\"statements\":80}}'",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:headless": "cypress run --headless",
    "test:e2e:chrome": "cypress run --browser chrome",
    "test:e2e:firefox": "cypress run --browser firefox",
    "test:e2e:edge": "cypress run --browser edge",
    "test:a11y": "cypress run --spec=\"cypress/e2e/**/*accessibility*.cy.js\"",
    "test:performance": "cypress run --spec=\"cypress/e2e/performance/*.cy.js\"",
    "test:smoke": "cypress run --spec=\"cypress/e2e/smoke/*.cy.js\"",
    "test:smoke:staging": "CYPRESS_BASE_URL=https://staging.example.com cypress run --spec=\"cypress/e2e/smoke/*.cy.js\"",
    "test:smoke:production": "CYPRESS_BASE_URL=https://production.example.com cypress run --spec=\"cypress/e2e/smoke/*.cy.js\"",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext js,jsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,css,md}",
    "type-check": "tsc --noEmit",
    "bundle-analyzer": "npx vite-bundle-analyzer dist",
    "lighthouse": "lhci autorun",
    "lighthouse:mobile": "lhci autorun --preset=mobile",
    "precommit": "npm run lint && npm run format:check && npm run test:unit",
    "prepare": "husky install || true",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "deploy:github": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "antd-mobile": "^5.28.1",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^30.0.0",
    "cypress": "^12.17.4",
    "cypress-axe": "^1.6.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "gh-pages": "^6.3.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.1",
    "jest-axe": "^7.0.1",
    "jest-environment-jsdom": "^29.4.1",
    "msw": "^1.1.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.3.0",
    "vite": "^4.1.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
